{
    "name": "anditsung/nova-resource-comment",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Anditsung\\NovaResourceComment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anditsung\\NovaResourceComment\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
